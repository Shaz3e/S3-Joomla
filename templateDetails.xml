	<config>
		<fields name="params">
				
			<!-- STYLE -->
            <fieldset name="STYLE">
				<field name="sitetitle" class="" type="text" default="" 
				label="Site Name" description="Type your sitename here, if its black the default will be use" />
				
				<field name="logo" class="" type="media" default="" 
				label="Logo Image" 
				description="You can use any image format as your logo" />
							
				<field name="" type="spacer" default="" label="Style Settings" description="If you would like to use any specific style please switch off Style Switcher" />
				
				<field name="style" class="" type="list" default="1" label="Choose Style" description="You can choose your default style here">
					<option value="1">style 1</option>
					<option value="2">style 2</option>
					<option value="3">style 3</option>
				</field>
				
			<field name="" type="spacer" default="" label="Front Page Settings" description="" />
			
				<field name="showFrontPage" type="list" default="1" 
					label="Show Frontpage Articles" 
					description="Show or Hide Front Page Article on Main Page if Yes Selected please enable Show main Body"> 
					<option value="0">No - Hide it</option>
					<option value="1">Yes - Show it</option>
				</field>
			
				<field name="showMainBody" type="list" default="1" 
					label="Show Main Body" 
					description="Show or Hide main body container on main page"> 
					<option value="0">No - Hide it</option>
					<option value="1">Yes - Show it</option>
				</field>

                <field name="hideComponent" type="menuitem" multiple="true"
                       label="Hide Component"
                       description="Hide Component (Body Container) on certain pages which are selected" />
            </fieldset>
			
			<fieldset name="LAYOUTS">
			
				<field name="copyright" class="" type="list" default="1" label="Show Copyright" description="You can Show or Hide copyright information here.">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				
				<field name="credit" class="" type="list" default="1" label="Show Credit" description="You can Show or Hide credit information here.">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>

				<field name="shaz3e" class="" type="list" default="1" label="Show Framework Logo" description="You can Show or Hide Framework Logo here.">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				
				<field name="" type="spacer" default="" label="Fixed Header and Menu" description="" />
				
				<field name="fixedHeader" class="" type="list" default="0" label="Fixed Header and Menu?" description="If you want to hide/show header and Menu on scroll up/down">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				
				<field name="fixedHeaderSize" class="" type="text" default="100" label="Enable Height" description="Enable Height i.e. 100 (Fixed header will be hide after scrolling down 100px)" 
				/>
				<field name="fixedHeaderSpeed" class="" type="text" default="400" label="Ease Speed" description="Define header ease slideUp/slideDown speed in ms default 400" />
			
			</fieldset>
			
			<!-- S3TEMPLATECONFIG -->
            <fieldset name="S3TEMPLATECONFIG">

				<field name="analytics" class="" type="text" default="" 
				label="Google Analytic" description="Type your Google Analytic code (UA-XXXXXXXX-X) here." />

				<field name="" type="spacer" default="" label="Development Options" description="" />
				
				<field name="ResponsiveVideo" class="" type="list" default="0" label="Use Responsive Video" description="">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				
				<field name="developmentMode" type="list" default="1" 
					label="Development Mode?" 
					description="Enable or Disable development mode for this website."> 
					<option value="0">Off</option>
					<option value="1">On</option>
				</field>
				
				<field name="LocalCDN" type="list" default="1" 
					label="js Hosted on CDN" 
					description="By disabling this feature template will use all js files locally which may cause slow down your website, we recommend to enable this feature for faster speed."> 
					<option value="0">Disable</option>
					<option value="1">Enable</option>
				</field>
				
				<field name="documentGenerator" class="" type="text" default="Shaz3e - The Revealing Paradigm"
				label="Type Meta Generator" description="Type your custom meta generator text here." />

            </fieldset>
		
		</fields>
	</config>
	
