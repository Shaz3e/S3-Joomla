		<fields name="params">
		
			<!-- S3TEMPLATECONFIG -->
            <fieldset name="S3TEMPLATECONFIG">
			
				<field name="sitetitle" class="" type="text" default="" 
				label="Site Name" description="Type your sitename here, if its blank the default will be use. This title will also be used in footer copyright." />
				
				<field name="color_theme" class="" type="list" default="1" 
					label="Choose Color Theme" 
					description="You can choose your default color theme here">
					<option value="1">Color 1</option>
					<option value="2">Color 2</option>
					<option value="3">Color 3</option>
					<option value="4">Color 4</option>
					<option value="5">Color 5</option>
					<option value="6">Color 6</option>
					<option value="7">Color 7</option>
					<option value="8">Color 8</option>
					<option value="0">Custom</option>
				</field>
				
				<field name="style" class="" type="list" default="0" label="Choose Style" description="You can choose your default style here">
					<option value="1">Style 1</option>
					<option value="2">Style 2</option>
					<option value="3">Style 3</option>
					<option value="4">Style 4</option>
					<option value="5">Style 5</option>
					<option value="6">Style 6</option>
					<option value="7">Style 7</option>
					<option value="8">Style 8</option>
					<option value="9">Style 9</option>
					<option value="10">Style 10</option>
					<option value="0">Custom</option>
				</field>

				<field name="analytics" class="" type="textarea" default="" 
					label="Analytic Code" description="Copy and Paste your javascript code" filter="raw" />
				
				<field name="developmentMode" type="radio" default="0" class="btn-group btn-group-yesno"
					label="Development Mode?" 
					description="Enable or Disable development mode for this website."> 
					<option value="0">Off</option>
					<option value="1">On</option>
				</field>
				
				<field name="fluidContainer" type="radio" default="0" class="btn-group btn-group-yesno"
					label="Fluid Layout" 
					description="Use Fluid or Static layout (both are responsive)"> 
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				
				<field name="documentGenerator" class="" type="text" default=""
				label="Type Meta Generator" description="Type your custom meta generator text here." />
				
				<field name="" type="spacer" default="" label="Front Page Settings" description="" />
			
				<field name="showFrontPage" type="radio" default="1" class="btn-group btn-group-yesno"
					label="Show Component" 
					description="Show or Hide component on Main Page if Yes Selected please enable Show main Body"> 
					<option value="0">No - Hide it</option>
					<option value="1">Yes - Show it</option>
				</field>
			
				<field name="showMainBody" type="radio" default="1" class="btn-group btn-group-yesno"
					label="Show Main Body" 
					description="Show or Hide main body container on main page"> 
					<option value="0">No - Hide it</option>
					<option value="1">Yes - Show it</option>
				</field>

                <field name="hideComponent" type="menuitem" multiple="true"
                       label="Hide Component"
                       description="Hide Component (Body Container) on certain pages which are selected" />
					   
            </fieldset>
			
			<!-- Header Settings -->
			<fieldset name="S3HEADERSETTINGS">
				<field name="logo" class="" type="media" default="" 
				label="Logo Image" 
				description="You can use any image format as your logo" />
				
				<field name="header_style" type="list" default="0"
					label="Change Header Style"
					description="Select the header you would like to use through out the website.">
					<option value="0">Default</option>
					<option value="1">Header 1</option>
					<option value="2">Header 2</option>
					<option value="3">Header 3</option>
					<option value="4">Header 4</option>
					<option value="5">Header 5</option>
					<option value="6">Top 1</option>
					<option value="7">Top 2</option>
					<option value="8">Top 3</option>
					<option value="9">Bottom 1</option>
					<option value="10">Bottom 2</option>
					<option value="11">Center 1</option>
					<option value="12">Center 2</option>
					<option value="13">Center 3</option>
				</field>
				
				<field name="s3_social_icon" type="text" default="" 
				label="Social Icons" description="Please enable or disable social icons in header by default its disabled, Remember to use Font Awesome library." filter="raw" />
	
				<field name="s3_phone_box" class="" type="text" default="" label="Type your Phone No" 
					description="Please type phone number here" filter="raw" />
					
				<field name="s3_email_box" class="" type="text" default="" label="Type your email" 
					description="Please type your email address here" />
					
				<field name="s3_calltoaction" class="" type="text" default="" label="Text for Call to Action" 
					description="Please write for call to action option" filter="raw" />
				
				<!-- Search Settings -->	
				<field name="" type="spacer" default="" label="Search Options" description="" />
				
				<field name="s3_search_box" type="radio" default="1" class="btn-group btn-group-yesno"
					label="Enable Search Box"
					description="Please enable or disable search box in header by default its enable">
					<option value="0">Diable</option>
					<option value="1">Enable</option>
				</field>
				
				<field name="s3_search_type" 
					type="list" 
					default="0" 
